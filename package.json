{
  "name": "ngx-meta",
  "workspaces": [
    "packages/@ngx-meta/*"
  ],
  "version": "0.0.0",
  "description": "Dynamic page title & meta tags utility for Angular (w/server-side rendering)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmitriydementor/ngx-meta.git"
  },
  "keywords": [],
  "author": {
    "name": "Dmytro K",
    "email": "dmitriydementor@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmitriydementor/ngx-meta/issues"
  },
  "homepage": "https://github.com/dmitriydementor/ngx-meta#readme",
  "scripts": {
    "clean": "rimraf dist",
    "build:core": "ts-node tools/build/packager.ts core",
    "build:core-new": "ng build @ngx-meta/core",
    "build": "npm run build:core-new",
    "lint": "tslint -p ./tsconfig.lint.json --force",
    "lint:check": "tslint-config-prettier-check ./tslint.lint.json",
    "rebuild": "npm run clean && npm run build",
    "ci:before": "greenkeeper-lockfile-update",
    "ci:after": "greenkeeper-lockfile-upload",
    "test": "jest --runInBand --colors",
    "test:ci": "jest --ci --updateSnapshot --colors",
    "release": "standard-version",
    "prepare": "husky"
  },
  "dependencies": {
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "lodash": "^4.17.15",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.13",
    "@angular-devkit/build-ng-packagr": "~0.901.13",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/jest": "^27.0.0",
    "@types/lodash": "^4.14.150",
    "@types/node": "^10.0.0",
    "angular-tslint-rules": "^1.20.4",
    "codelyzer": "^5.2.2",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^9.0.11",
    "jest": "^27.0.0",
    "jest-junit-reporter": "^1.1.0",
    "jest-preset-angular": "9.0.7",
    "lerna": "^8.1.2",
    "lint-staged": "^10.0.0",
    "ng-packagr": "^9.0.0",
    "prettier": "1.19.1",
    "prettier-tslint": "^0.4.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.0",
    "tsickle": "^0.38.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
